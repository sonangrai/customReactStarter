{
  "env": {
    "development": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": [["babel-plugin-styled-components", { "displayName": true }]]
    },
    "production": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": [
        [
          "babel-plugin-styled-components",
          { "ssr": true, "displayName": false, "minify": true }
        ]
      ]
    }
  }
}
